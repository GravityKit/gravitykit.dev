{
  "buildCommand": "npm run docs:full",
  "outputDirectory": "build",
  "cleanUrls": true,
  "trailingSlash": false,
  "redirects": [
    {
      "source": "/docs",
      "destination": "/docs/",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}