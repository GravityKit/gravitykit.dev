{
  "_comment": "Set WP_PLUGINS_DIR environment variable to override wp_plugins_dir for local regeneration",
  "wp_plugins_dir": "",
  "defaults": {
    "ignoreFiles": [
      "**/vendor/**",
      "**/vendor_prefixed/**",
      "**/node_modules/**",
      "**/tests/**",
      "**/test/**",
      "**/.git/**",
      "**/build/**",
      "**/dist/**",
      "**/tmp/**",
      "**/temp/**",
      "**/*.min.js",
      "**/*.min.css"
    ],
    "ignoreHooks": [
      "deprecated_*",
      "private_*",
      "_internal_*",
      "gk/foundation*"
    ],
    "customFields": {
      "since": true,
      "deprecated": true,
      "internal": false,
      "example": true
    },
    "outputStructure": {
      "actions": true,
      "filters": true,
      "byFile": true,
      "byFeature": false
    }
  },
  "plugins": {
    "gravityview-advanced-filtering": {
      "wp_plugin_dir": "gravityview-advanced-filter"
    },
    "gravityview-dashboard-views": {
      "wp_plugin_dir": "gravityview-dashboard"
    },
    "gravityview-social-sharing-seo": {
      "wp_plugin_dir": "gravityview-social-sharing-seo"
    },
    "calendar": {
      "wp_plugin_dir": "Calendar"
    }
  },
