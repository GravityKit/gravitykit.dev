{
  "$schema": "./repos-config.schema.json",
  "reposDir": "./repos",
  "outputDir": "./docs",
  "defaults": {
    "branch": "develop",
    "ignoreFiles": [
      "**/vendor/**",
      "**/vendor_prefixed/**",
      "**/node_modules/**",
      "**/tests/**",
      "**/test/**",
      "**/.git/**",
      "**/build/**",
      "**/dist/**",
      "**/tmp/**",
      "**/temp/**",
      "**/*.min.js",
      "**/*.min.css"
    ],
    "ignoreHooks": [
      "deprecated_*",
      "private_*",
      "_internal_*",
      "gk/foundation*"
    ],
    "customFields": {
      "since": true,
      "deprecated": true,
      "internal": false,
      "example": true
    }
  },
  "categories": {
    "gravityview": {
      "label": "GravityView",
      "position": 1
    },
    "gravityview-extensions": {
      "label": "Extensions",
      "parent": "gravityview",
      "position": 2
    },
    "gravityview-layouts": {
      "label": "Layouts",
      "parent": "gravityview",
      "position": 3
    },
    "gravitykit": {
      "label": "GravityKit Products",
      "position": 2
    },
    "gravity-forms": {
      "label": "Gravity Forms Add-Ons",
      "position": 3
    }
  },
