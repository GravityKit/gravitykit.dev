{
  "$schema": "./repos-config.schema.json",
  "reposDir": "./repos",
  "outputDir": "./docs",
  "defaults": {
    "branch": "develop",
    "ignoreFiles": [
      "**/vendor/**",
      "**/vendor_prefixed/**",
      "**/node_modules/**",
      "**/tests/**",
      "**/test/**",
      "**/.git/**",
      "**/build/**",
      "**/dist/**",
      "**/tmp/**",
      "**/temp/**",
      "**/*.min.js",
      "**/*.min.css"
    ],
    "ignoreHooks": [
      "deprecated_*",
      "private_*",
      "_internal_*",
      "gk/foundation*"
    ],
    "customFields": {
      "since": true,
      "deprecated": true,
      "internal": false,
      "example": true
    }
  },
  "products": [
    {
      "id": "gravityactions",
      "repo": "GravityKit/GravityActions",
      "label": "GravityActions",
      "routeBasePath": "docs/gravityactions"
    },
